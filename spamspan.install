/**
 * Remove obsolete Drupal variables.
 */
function spamspan_update_1000() {

  // Delete variables.
  update_variable_del('spamspan_at');
  update_variable_del('spamspan_domainclass');
  update_variable_del('spamspan_userclass');
  update_variable_del('spamspan_use_graphic');
}
